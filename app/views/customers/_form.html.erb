<%= form_for @customer do |f| %>
<p>
  <%= f.label 'Family Name' %>
  <%= f.text_field :family_name, class: 'form_controll' %>
</p>
<p>
  <%= f.label 'Given Name' %>
  <%= f.text_field :given_name, class: 'form_controll' %>
</p>
<p>
  <%= f.label 'E-mail' %>
  <%= f.text_field :email, class: 'form_controll' %>
</p>
<%= f.submit 'Reigister' %>
<% end %>
